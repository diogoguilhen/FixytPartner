<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="fixyt.fixytMotor.MainPartner"
    tools:showIn="@layout/app_bar_main_partner">


  <Button
      android:id="@+id/botaoAuxilio"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentLeft="true"
      android:layout_alignParentStart="true"
      android:text="Atendimentos Emergenciais"
      tools:layout_editor_absoluteX="0dp"
      tools:layout_editor_absoluteY="56dp" />

    <Button
        android:text="Perfil"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/botaoPerfil"
        android:layout_alignBaseline="@+id/botaoLogout"
        android:layout_alignBottom="@+id/botaoLogout"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="28dp"
        android:layout_marginEnd="28dp" />






</android.support.constraint.ConstraintLayout>
